{{#each items}}
    <div>
        <p> {{this.type}}</p>
        <{{this.type}} href="{{this.href}}" data-section="{{this.key}}" {{#ifEquals @index 0}} class="active" {{/ifEquals}}>
            {{this.textContent}}
        </{{this.type}}>
        {{#ifEquals key 'registration'}} 
            <div class="navbar__border-left"></div>
        {{/ifEquals}}
    </div>
{{/each}}