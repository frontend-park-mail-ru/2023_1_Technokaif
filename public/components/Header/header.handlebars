<div class="{{formHeader}}">
    <a class="{{title}}">
        <img class="title__img" src="{{logoSrc}}">
        <p class="{{titleClass}}">{{titleName}}</p>
    </a>
    {{#if errorTop}}
        <p hidden class="{{errorText}}"></p>
    {{/if}}
    <div class="{{notificationClass}}">
        <p>{{notificationName}}</p>
    </div>
    <div class="{{descriptionClass}}">
        <p>{{descriptionName}}</p>
    </div>
</div>
