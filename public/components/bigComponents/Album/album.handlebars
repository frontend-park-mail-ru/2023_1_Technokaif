<div class="{{mainDiv}} js__album">
    <div class="{{headerDiv}}">
        <div class="{{headerImgClass}}">
            <img class="{{imgClass}}" src="{{imgSrc}}">
        </div>
        <div class="{{headerDescriptions}}">
            <div class="{{headerNameDiv}}">
                <p class="{{headerNameClass}}">
                    {{headerName}}
                </p>
            </div>

            <div class="{{headerNameOfElementDiv}}">
                <p class="{{headerNameOfElementClass}}">
                    {{headerNameOfElement}}
                </p>
            </div>

            <div class="{{headerDescription}}">
                <div class="{{ArtistDiv}}">
                        <p class="{{byClass}}">By </p>
                        <p class="{{ArtistClass}}">{{ArtistName}}</p>
                </div>
                <div class="{{DescriptionsDiv}}">
                    <p class="{{DescriptionsClass}}"> {{Descriptions}}</p>
                </div>
            </div>




        </div>
    </div>
    <div class="{{ButtonsDiv}}">
        <img class="{{buttonPlay}}"src="/static/svg/play.svg">
        <img class="{{albumLike}}" src="/static/svg/notLiked.svg">
    </div>

    <div class="{{bottomDiv}}">
        <div class="{{numberDiv}}">
            <p>#</p>
        </div>
        <div class="{{TitleDiv}}">
            <p>TITLE</p>
        </div>
        <div class="{{liked}}">
        </div>
        <div class="{{ListensDiv}}">
            <p>LISTENS</p>
        </div>
    </div>

    <div class="{{placementTracks}}"></div>

</div>
