<div class="navbar js__navbar">
    <div class="navbar_library_element">
    </div>
    {{#each items}}
        <div class="navbar__{{this.key}}__item">
            {{#ifEquals this.key 'registration'}}
                <div class="navbar__border-left"></div>
            {{/ifEquals}}

            <{{this.type}} href="{{this.href}}" data-section="{{this.key}}" class="{{#ifNotEquals key 'registration'}}
                navbar__{{this.key}}
            {{/ifNotEquals}}
            {{#ifAllEquals this.key 'registration' 'login'}} navbar__link {{/ifAllEquals}}
            {{#ifAllEquals this.key 'profile' 'logout'}} navbar__button {{/ifAllEquals}}
            {{#ifEquals this.key 'premium'}}
                {{#ifEquals ../name 'unAuthNavbar'}}
                    navbar__link
                {{else}}
                    navbar__button
                {{/ifEquals}}
            {{/ifEquals}}">
                {{this.name}}
            </{{this.type}}>
        </div>
    {{/each}}
    <div class="js__avatar-placement"> </div>
</div>
